{
  "name": "@cordystackx/cordy_minikit",
  "version": "1.5.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "description": "Cordy Minikit - Open Source Web3 Dev Toolkit (Wallet Connect, Tx, Disconnect, Dashboard)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && pnpm run copy-css",
    "copy-css": "mkdir -p dist/css/UI_Comp && cp css/UI_Comp/styles.module.css dist/css/UI_Comp/",
    "release": "pnpm publish --access public"
  },
  "keywords": [
    "web3",
    "nextjs",
    "react",
    "wagmi",
    "viem",
    "ethers",
    "blockchain",
    "wallet-connect",
    "npm-package",
    "cordy-minikit"
  ],
  "author": "Marc Giestin Louis Cordova",
  "license": "MIT",
  "dependencies": {
    "@coinbase/wallet-sdk": "4.3.7",
    "@tanstack/react-query": "^5.90.5",
    "ethers": "^6.15.0",
    "next": "^16.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "wagmi": "^2.18.2"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2"
  }
}
